# Registry configuration
REGISTRY_DOMAIN=registry.example.com

# S3 storage driver configuration
REGISTRY_STORAGE_S3_ACCESSKEY=key
REGISTRY_STORAGE_S3_SECRETKEY=secret
REGISTRY_STORAGE_S3_BUCKET=bucket
REGISTRY_STORAGE_S3_REGION=region
REGISTRY_STORAGE_S3_REGIONENDPOINT=endpoint
REGISTRY_STORAGE_S3_SECURE=true

# Auth configuration
REGISTRY_BASIC_AUTH_USERNAME=user
REGISTRY_BASIC_AUTH_PASSWORD=password

